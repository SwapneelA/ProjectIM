<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.gasagency.pojo">
	<class name="DeliveryPersonMaster" table="DeliveryPersonMaster">
 	
 		<id name="deliveryPersonId" column="DeliveryPersonId">
 			<generator class="increment"></generator>
 		</id>
 	
 		<property name="firstName" column="DeliveryPersonFirstName"/>
 		<property name="lastName" column="DeliveryPersonLastName"/>
 		<property name="phoneNumber" column="PhoneNumber" />

 
 	</class>
	
</hibernate-mapping>
