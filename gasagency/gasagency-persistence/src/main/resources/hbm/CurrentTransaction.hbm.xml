<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
      
<hibernate-mapping package="com.gasagency.pojo">
	<class name="CurrentTransaction" table="cylindertransaction">
		<id name="id" column="Id">
			<generator class="increment" />
		</id>
		<property name="cylinderTypeId" column="CylinderTypeId" />
		<property name="categoryId" column="VolumeId" />
		<property name="operationTypeId" column="operationTypeId" />
		<property name="deliveryPersonId" column="DeliveryPersonId" />
		<property name="quantity" column="Quantity" />
		<property name="createDate" column="CreateDate" />
		<property name="transactionId" column="TransactionId" />
	</class>
</hibernate-mapping>