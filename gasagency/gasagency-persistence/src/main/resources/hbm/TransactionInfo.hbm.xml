<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.gasagency.pojo">
<class name="TransactionInfo" table="transactioninfo">
	<id name="id" column="id">
		<generator class="increment" />
	</id>
	<property name="transactionDate" column="transactiondate"></property>
	<property name="openTransaction" column="OpenTransaction" />
	<property name="updateDate" column="UpdateDate" />		
</class>
</hibernate-mapping>